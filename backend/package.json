{
  "name": "radiology-textbook-backend",
  "version": "1.0.0",
  "description": "Backend API for the Radiology Textbook Platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:unit": "jest src/__tests__/unit",
    "test:integration": "jest src/__tests__/integration",
    "test:e2e": "jest src/__tests__/e2e",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "migrate": "ts-node scripts/migrate.ts",
    "seed": "ts-node scripts/seed.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "radiology",
    "education",
    "textbook",
    "medical",
    "learning",
    "typescript",
    "express",
    "postgres"
  ],
  "author": "AI Coding Assistant",
  "license": "MIT",
  "dependencies": {
    "@neondatabase/serverless": "^0.4.3",
    "@types/multer": "^2.0.0",
    "bcrypt": "^5.1.0",
    "better-auth": "^0.2.1",
    "compression": "^1.8.1",
    "cornerstone-wado-image-loader": "^4.13.2",
    "cors": "^2.8.5",
    "dcmjs": "^0.47.1",
    "dicom-parser": "^1.8.21",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "multer": "^2.0.2",
    "openai": "^6.16.0",
    "pg": "^8.11.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
