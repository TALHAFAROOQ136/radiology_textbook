{
  "name": "radiology-textbook-platform",
  "version": "1.0.0",
  "description": "AI-native radiology textbook platform with Docusaurus, RAG chatbot, and authentication",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run start",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "tsc -p backend/tsconfig.json",
    "test": "jest && cd backend && pytest tests/",
    "test:frontend": "jest",
    "test:backend": "cd backend && pytest tests/",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [
    "docusaurus",
    "radiology",
    "education",
    "ai",
    "textbook"
  ],
  "author": "Medical Education Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@docusaurus/core": "^3.1.0",
    "@docusaurus/preset-classic": "^3.1.0",
    "@docusaurus/module-type-aliases": "^3.1.0",
    "better-auth": "^0.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "openai": "^4.24.7",
    "@qdrant/js-client-rest": "^1.16.2",
    "@neondatabase/serverless": "^0.4.3",
    "fastify": "^4.0.0",
    "zod": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}